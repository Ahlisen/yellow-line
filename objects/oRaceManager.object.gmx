            <kind>1</kind>
            <string>///Setup drivers
raceStarted = false;
raceFinished = false;
backgroundMusic = noone;
songEnding = false;

var players = 2;
var vehicles = 2;

for(var i = 0; i&lt;vehicles; i++) {
    var startDirection = -45;
    var car = noone;
    var correction = floor(vehicles/2)-0.5*((vehicles+1) mod 2);
    
    var startlineX = lengthdir_x((i-correction)*16, startDirection+90);
    var startlineY = lengthdir_y((i-correction)*16, startDirection+90);
    
    var carType = choose(oTransam, oHoverCraft, oGenericCar, oBike);
    car = instance_create(startlineX, startlineY, carType);
    
    if(players &gt; 0) {
        car.player = i;
        car.isPlayer = true;
        car.state = player_wait;
        players--;
        //car.engine = audio_play_sound(aSinus, 0, true);
    }
    
    car.phy_rotation = -startDirection;
}
//alarm[0] = room_speed*2;
</string>
